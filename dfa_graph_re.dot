digraph DFA {
  rankdir=LR;  // 横向布局

 node [shape = circle];   // 初始状态

q6[shape = doublecircle];
  q0 [label="State q0\n(startState)"];
  q1 [label="State q1"];
  q2 [label="State q2"];
  q3 [label="State q3"];
  q4 [label="State q4"];
  q5 [label="State q5"];
  q6 [label="State q6\n(endState)"];

  q0 -> q1 [label="a"];
  q1 -> q2 [label="a"];
  q1 -> q3 [label="b"];
  q1 -> q4 [label="c"];
  q2 -> q5 [label="b"];
  q3 -> q2 [label="a"];
  q3 -> q3 [label="b"];
  q3 -> q4 [label="c"];
  q4 -> q2 [label="a"];
  q4 -> q3 [label="b"];
  q4 -> q4 [label="c"];
  q5 -> q6 [label="c"];
}
