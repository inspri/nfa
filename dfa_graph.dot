digraph DFA {
  rankdir=LR;  // 横向布局

 node [shape = circle];   // 初始状态

q4[shape = doublecircle];
  q0 [label="State q0\n(startState)"];
  q1 [label="State q1"];
  q2 [label="State q2"];
  q3 [label="State q3"];
  q4 [label="State q4\n(endState)"];

  q0 -> q1 [label="0"];
  q0 -> q2 [label="1"];
  q1 -> q1 [label="0"];
  q1 -> q3 [label="1"];
  q2 -> q1 [label="0"];
  q2 -> q2 [label="1"];
  q3 -> q1 [label="0"];
  q3 -> q4 [label="1"];
  q4 -> q1 [label="0"];
  q4 -> q2 [label="1"];
}
